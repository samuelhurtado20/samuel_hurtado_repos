{
    "compilerOptions": {
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "lib": ["esnext"],
      "esModuleInterop": true,
      "target": "es2016",
      "module": "commonjs",
      "forceConsistentCasingInFileNames": true,
      "noUnusedLocals": true, 
      "noUnusedParameters": true,  
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true, 
      "skipLibCheck": true,
      "resolveJsonModule": true
    }
  }